{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    },
    "test": {
      "outputs": ["coverage/"]
    },
    "build": {
      "dependsOn": ["^build", "typecheck"],
      "outputs": [".next/", "dist/", "build/"]
    },
    "e2e": {
      "dependsOn": ["build"],
      "outputs": [".playwright/", "test-results/"]
    }
  }
}
