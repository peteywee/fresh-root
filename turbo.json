{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "apps/web/.next/**",
        "!apps/web/.next/cache/**",
        "packages/**/dist/**",
        "functions/lib/**"
      ]
    },
    "test": {
      "dependsOn": ["^build"],
      "inputs": ["app/**", "src/**", "lib/**", "vitest.config.ts", "vitest.setup.ts"]
    },
    "@fresh-root/markdown-fixer#test": {
      "cache": false,
      "outputs": []
    },
    "@fresh-schedules/types#test": {
      "cache": false,
      "outputs": []
    },
    "lint": {},
    "typecheck": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {}
  }
}
