{
  "name": "fresh-root",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "clean": "turbo run clean && rm -rf node_modules",
    "prepare": "pnpm run enforce-pnpm",
    "enforce-pnpm": "node scripts/enforce-pnpm.js",
    "// --- OPS ---": "",
    "deploy:rules": "firebase deploy --only firestore:rules,storage",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:hosting": "firebase deploy --only hosting",
    "// --- SDK ---": "",
    "build:sdk": "pnpm --filter \"@fresh-schedules/api-framework\" build",
    "release:series-a": "node ./scripts/release-series-a.mjs"
  },
  "engines": {
    "node": ">=20.10.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.12.1",
  "devDependencies": {
    "@types/ioredis": "^5.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitest/ui": "^4.0.14",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.5",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globby": "^16.0.0",
    "husky": "^9.1.7",
    "prettier": "^3.7.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "lucide-react": "^0.460.0",
    "@tanstack/react-query": "^5.90.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "firebase-admin": "^13.6.0",
    "firebase-functions": "^7.0.0",
    "ioredis": "^5.8.2",
    "next": "^16.0.5",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.5",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "turbo": "^2.6.1",
    "zod": "^4.1.13"
  },
  "pnpm": {
    "overrides": {
      "@iac-fresh/eslint-config": "npm:eslint-config-next@latest",
      "@iac-fresh/prettier-config": "npm:prettier@latest"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "firebaseui>firebase": "12.x"
      }
    }
  },
  "peerDependencies": {
    "@types/ioredis": "^5.0.0"
  }
}
