{
  "name": "fresh-root",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "clean": "turbo run clean && rm -rf node_modules",
    "prepare": "pnpm run enforce-pnpm",
    "enforce-pnpm": "node scripts/enforce-pnpm.js",
    "// --- OPS ---": "",
    "deploy:rules": "firebase deploy --only firestore:rules,storage",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:hosting": "firebase deploy --only hosting",
    "// --- SDK ---": "",
    "build:sdk": "pnpm --filter \"@fresh-schedules/api-framework\" build",
    "release:series-a": "node ./scripts/release-series-a.mjs",
    "// --- TEST INTELLIGENCE ---": "",
    "test:intelligence": "cd tests/intelligence && pnpm test:intelligence",
    "test:intelligence:quick": "cd tests/intelligence && pnpm test:intelligence:quick",
    "test:ai-demo": "cd tests/intelligence && pnpm demo",
    "test:auto-generate": "cd tests/intelligence && pnpm test:auto-generate",
    "test:performance": "cd tests/intelligence && pnpm test:performance",
    "test:contracts": "cd tests/intelligence && pnpm test:contracts",
    "test:mutation": "cd tests/intelligence && pnpm test:mutation",
    "test:chaos": "cd tests/intelligence && pnpm test:chaos",
    "test:analytics": "cd tests/intelligence && pnpm test:analytics"
  },
  "engines": {
    "node": ">=20.10.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.12.1",
  "devDependencies": {
    "@types/ioredis": "^5.0.0",
    "@types/node": "^24.10.1",
    "@types/papaparse": "^5.3.15",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/speakeasy": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@vitest/ui": "^4.0.14",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.5",
    "eslint-plugin-unused-imports": "^4.3.0",
    "firebaseui": "^6.0.0",
    "globby": "^16.0.0",
    "husky": "^9.1.7",
    "papaparse": "^5.4.1",
    "prettier": "^3.7.1",
    "qrcode": "^1.5.4",
    "rimraf": "^6.0.1",
    "speakeasy": "^2.0.0",
    "tailwindcss": "^4.1.17",
    "tsup": "8.5.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.14",
    "xlsx": "^0.18.5",
    "zustand": "4.5.2"
  },
  "dependencies": {
    "@fresh-schedules/types": "workspace:*",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.207.0",
    "@opentelemetry/sdk-node": "^0.207.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@sentry/nextjs": "^10.25.0",
    "@tanstack/react-query": "^5.90.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "firebase": "^12.0.0",
    "firebase-admin": "^13.6.0",
    "firebase-functions": "^7.0.0",
    "globals": "^16.5.0",
    "ioredis": "^5.8.2",
    "lucide-react": "^0.460.0",
    "next": "^16.0.5",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.5",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "turbo": "^2.6.1",
    "zod": "^4.1.13"
  },
  "pnpm": {
    "overrides": {
      "@fresh-schedules/types": "link:packages/types",
      "lucide-react": "npm:lucide-react@latest",
      "@iac-fresh/eslint-config": "npm:eslint-config-next@latest",
      "@iac-fresh/prettier-config": "npm:prettier@latest",
      "next": "16.0.5"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "firebaseui>firebase": "12.x"
      }
    }
  },
  "peerDependencies": {
    "@types/ioredis": "^5.0.0"
  }
}
