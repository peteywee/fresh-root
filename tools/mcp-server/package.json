{
  "name": "fresh-schedules-mcp-server",
  "version": "2.0.0",
  "description": "MCP server for Fresh Schedules repo management with indexing",
  "type": "module",
  "main": "dist/index.js",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:gpt": "tsx src/gpt-server.ts",
    "dev": "tsx watch src/index.ts",
    "dev:gpt": "tsx watch src/gpt-server.ts",
    "test": "tsx src/test.ts",
    "test:tools": "tsx src/test-tools.ts",
    "inspect": "pnpm dlx @modelcontextprotocol/inspector"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "glob": "^10.3.10",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18"
  }
}
